{% for testimonial in testimonials %}
<blockquote{% if loop.index == testimonials|length %} style="margin-bottom: 0;"{% endif %}><i class="icon-large icon-quote-left icon-muted"></i>{{ testimonial.translation.content|striptags }}<i class="icon-large icon-quote-right icon-muted"></i><small>{{ testimonial.name }}</small>
</blockquote>
{% endfor %}
